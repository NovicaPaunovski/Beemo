{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BEEMO_DB_CONTEXT_CONNECTION": "Server=localhost;Port=3306;Database=Beemo;UID=root;PWD=Password123"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5015"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BEEMO_DB_CONTEXT_CONNECTION": "Server=localhost;Port=3306;Database=Beemo;UID=root;PWD=Password123"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7059;http://localhost:5015"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BEEMO_DB_CONTEXT_CONNECTION": "Server=localhost;Port=3306;Database=Beemo;UID=root;PWD=Password123"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "BEEMO_DB_CONTEXT_CONNECTION": "Server=host.docker.internal;Port=3306;Database=Beemo;UID=root;PWD=Password123"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:17545",
      "sslPort": 44356
    }
  }
}